{
  "name": "acchal-electron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.13.0",
    "@blueprintjs/icons": "^3.6.0",
    "@blueprintjs/table": "^3.4.1",
    "electron": "^4.0.5",
    "electron-devtools-installer": "^2.2.4",
    "electron-store": "^3.2.0",
    "foreman": "^3.0.1",
    "fs": "0.0.1-security",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-responsive": "^6.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.5",
    "react-youtube": "^7.9.0",
    "styled-components": "^4.1.3",
    "typescript": "^3.4.4",
    "video.js": "^7.4.1"
  },
  "main": "src/electron-starter.js",
  "homepage": "./home",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "dev": "nf start",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Acchallenge --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . Acchallenge --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron-packager": "^13.1.0"
  }
}
